{
  "expo": {
    "name": "ClassHub",
    "slug": "classhub",
    "scheme": "ClassHub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#A7EA9D"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cobynnha.nofrills",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.207695360729-tfodeknp9geq8o55e2pb3bmg3pj5097p"
        }
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#A7EA9D"
      },
      "package": "com.cobynnha.nofrills",
      "permissions": [
        "android.permissions.INTERNET"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.207695360729-tfodeknp9geq8o55e2pb3bmg3pj5097p"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "f8d30428-d531-4dae-b2c6-87b4a9a7723e"
      },
      "router": {
        "origin": false
      }
    },
    "owner": "caiaocesar"
  }
}
